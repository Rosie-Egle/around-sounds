{
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": ["dom", "esnext"],
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    // "strictNullChecks": true,
    "baseUrl": "./src",
    "paths": {
      "components/*": ["components/*"],
      "clients/*": ["clients/*"]
    }
  },
  "include": ["src", "env.d.ts"]
}
